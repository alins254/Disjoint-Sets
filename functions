

#include <stdio.h>
#include <stdlib.h>

typedef struct node{
    int val;
    int size;
    struct node* parent;
}NodeT;

NodeT* makeSet(int x){
    NodeT* n = (NodeT*)calloc(sizeof(NodeT),1);
    n->val=x;
    n->size=0;
    n->parent=n;
    return n;
}

NodeT* findSet(NodeT* x){
    if(x!=x->parent)
        x->parent = findSet(x->parent);
    return x;
}

void unionSet (NodeT* x, NodeT* y)
{
    if(NULL==x||NULL==y)
        return;
    NodeT* xRoot = findSet(x);
    NodeT* yRoot = findSet(y);
    if(xRoot->size>=yRoot->size){
        yRoot->parent=xRoot;
        xRoot->size++;
    }
    else{
        xRoot->parent=yRoot;
        yRoot->size++;
    }
}



int main()
{
    printf("Hello World");
    
    return 0;
}
